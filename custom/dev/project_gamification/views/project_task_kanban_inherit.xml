<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_task_kanban_inherit_upload_button" model="ir.ui.view">
        <field name="name">project.task.kanban.inherit.upload.button</field>
        <field name="model">project.task</field>
        <field name="inherit_id" ref="project.view_task_kanban"/>
        <field name="arch" type="xml">

            <xpath expr="//div[hasclass('o_kanban_record_body')]" position="after">
                <t t-call="my.custombuttontest1"/>
            </xpath>

        </field>
    </record>
</odoo>

